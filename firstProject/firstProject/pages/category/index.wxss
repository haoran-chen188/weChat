page {
  height: 100%;
}
.cats {
  height: 100%;
}
.cats .cats_container {
  height: calc(100vh - 90rpx);
  display: flex;
}
.cats .cats_container .left_menu {
  flex: 2;
  flex-wrap: wrap;
}
.cats .cats_container .left_menu .catItems {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cats .cats_container .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cats .cats_container .right_containt {
  flex: 5;
}
.cats .cats_container .right_containt .good_group .cat_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cats .cats_container .right_containt .good_group .cat_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cats .cats_container .right_containt .good_group .good_list {
  display: flex;
  flex-wrap: wrap;
}
.cats .cats_container .right_containt .good_group .good_list navigator {
  width: 33.33%;
}
.cats .cats_container .right_containt .good_group .good_list navigator image {
  width: 50%;
}
